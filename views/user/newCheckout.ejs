                                        <% if(locals.cartData && cartData.product.length>0) { %>
                                            <% let total=0 %>
                                                <% for(let i=0; i<cartData.product.length; i++) { %>
                                                    <tr>
                                                        <td>
                                                            <a
                                                                href="/productDetails?id=<%= cartData.product[i].productId._id %>">
                                                                <img src="/public/uploads/<%= cartData.product[i].productId.images[0] %>"
