                                                            </a>
                                                        </td>
                                                        <td>
                                                            <h5><a
                                                                    href="/productDetails?id=<%= cartData.product[i].productId._id %>">
                                                                    <%= cartData.product[i].productId.productName %>
                                                                </a></h5>
                                                            <span>x <%= cartData.product[i].quantity %></span>
                                                        </td>
                                                        <td>₹ <%= cartData.product[i].productId.price *
                                                                cartData.product[i].quantity %>
                                                        </td>
                                                    </tr>
                                                    <% total +=cartData.product[i].productId.price *
                                                        cartData.product[i].quantity %>
                                                        <% } %>
                                                            <tr>
                                                                <th>Total</th>
                                                                <td><span id="totalPrice">₹ <%= total.toFixed(2) %>
                                                                    </span></td>
                                                            </tr>
                                                            <% } %>
                                    </tbody>
